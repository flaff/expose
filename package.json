{
  "name": "expose",
  "private": true,
  "description": "A simple portfolio",
  "version": "0.1.0",
  "author": "Michal Cziomer, flaff <michal.cziomer@gmail.com>",
  "dependencies": {
    "bootstrap": "4.5.0",
    "gatsby": "2.22.9",
    "gatsby-image": "2.4.5",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-linaria": "2.0.0",
    "gatsby-plugin-manifest": "2.4.9",
    "gatsby-plugin-no-sourcemaps": "2.2.0",
    "gatsby-plugin-offline": "3.2.7",
    "gatsby-plugin-react-helmet": "3.3.2",
    "gatsby-plugin-sharp": "2.6.9",
    "gatsby-source-filesystem": "2.3.8",
    "gatsby-transformer-sharp": "2.5.3",
    "linaria": "2.0.0-alpha.5",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-bootstrap": "1.0.1",
    "react-bootstrap-icons": "1.0.2-alpha3",
    "react-collapsible": "2.8.0",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-image-lightbox": "5.1.1",
    "react-photo-gallery": "8.0.0",
    "react-use-scroll-position": "2.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.8.0",
    "@testing-library/react": "10.0.4",
    "babel-jest": "26.0.1",
    "babel-preset-gatsby": "0.4.7",
    "bundlesize": "0.18.0",
    "bundlewatch": "0.2.7",
    "gatsby-cli": "2.12.34",
    "gh-pages": "2.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop --host=0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "deploy": "gh-pages -d public -r https://$GITHUB_TOKEN@github.com/flaff/expose.git",
    "clear-win": "rimraf .cache & rimraf .linaria-cache & rimraf public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "bundlewatch": {
    "files": [
      {
        "path": "public/**/*.js",
        "maxSize": "200kB"
      },
      {
        "path": "public/**/*.css",
        "maxSize": "200kB"
      }
    ]
  }
}
