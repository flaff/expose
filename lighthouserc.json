{
  "ci": {
    "collect": {
      "url": ["http://localhost"],
      "numberOfRuns": 2,
      "staticDistDir": "./public"
    }
  },
  "assert": {
    "preset": "lighthouse:no-pwa",
    "uses-long-cache-ttl": "off",
    "uses-http2": "off"
  },
  "upload": {
    "target": "temporary-public-storage"
  }
}
